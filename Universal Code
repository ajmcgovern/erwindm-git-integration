/* erwin Builtin Trigger */
/* UPDATE trigger on Bk Auth */
BEGIN
  DECLARE  @numrows int,
           @nullcnt int,
           @validcnt int,
           @insAuth_Id char(9), 
           @insBk_Id char(9),
