    /* ERWIN_RELATION:CHECKSUM="00000000", PARENT_OWNER="dbo", PARENT_TABLE="Publshr"
    CHILD_OWNER="dbo", CHILD_TABLE="Bk"
    P2C_VERB_PHRASE="", C2P_VERB_PHRASE="", 
