        NOT EXISTS (
          SELECT * FROM dbo.Personal Chk
          WHERE
            /* %JoinFKPK(dbo.Personal Chk,dbo.Pmt," = "," AND") */
            dbo.Personal Chk.Pmt_Nbr = dbo.Pmt.Pmt_Nbr
        )
