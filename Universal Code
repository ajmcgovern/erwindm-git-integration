      SELECT * FROM deleted,dbo.Publshr Logo
      WHERE
        /*  %JoinFKPK(dbo.Publshr Logo,deleted," = "," AND") */
        dbo.Publshr Logo.Publshr_Id = deleted.Publshr_Id
    )
    BEGIN
      SELECT @errno  = 30001,
             @errmsg = 'Cannot delete dbo.Publshr because dbo.Publshr Logo exists.'
      GOTO error
